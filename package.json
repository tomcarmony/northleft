{
  "name": "Boilerplate",
  "version": "0.0.2",
  "description": "New project boilerplate",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git://github.com/bradcerasani/boilerplate.git"
  },
  "browser": {
    "plugin": "./src/javascript/vendor/jquery-plugin.js"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "plugin": {
      "depends": [
        "jquery:$"
      ]
    }
  },
  "devDependencies": {
    "browser-sync": "^1.3.5",
    "browserify": "~4.x",
    "browserify-shim": "~3.6.0",
    "connect": "~2.25.7",
    "gulp": "~3.8.7",
    "gulp-autoprefixer": "0.0.8",
    "gulp-changed": "~1.0.0",
    "gulp-file-include": "^0.4.2",
    "gulp-imagemin": "~1.0.0",
    "gulp-notify": "~1.5.0",
    "gulp-open": "~0.2.8",
    "gulp-rename": "^1.2.0",
    "gulp-sass": "^0.7.3",
    "gulp-streamify": "0.0.5",
    "gulp-uglify": "^0.3.1",
    "gulp-util": "^3.0.0",
    "pretty-hrtime": "^0.2.1",
    "vinyl-source-stream": "~0.1.1",
    "watchify": "^1.0.2"
  },
  "dependencies": {
    "browserify": "^4.1.8",
    "browserify-shim": "^3.5.0",
    "connect": "^2.19.4",
    "gulp": "^3.7.0",
    "gulp-changed": "^0.4.0",
    "gulp-imagemin": "^0.6.0",
    "gulp-notify": "^1.3.1",
    "gulp-sass": "^0.7.3",
    "gulp-streamify": "0.0.5",
    "jquery": "~2.1.1"
  }
}
